{
  "name": "vue-lims-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --fix --ext .js,.vue src",
    "dev": "vue-cli-service serve"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.4.1",
    "@tinymce/tinymce-vue": "1.1.0",
    "axios": "^0.18.0",
    "echarts": "^4.2.0-rc.2",
    "element-ui": "^2.4.8",
    "nprogress": "^0.2.0",
    "screenfull": "^3.3.3",
    "tinymce": "4.8.3",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vue-scrollbar": "^1.0.4",
    "vuedraggable": "^2.16.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "@vue/eslint-config-airbnb": "^3.0.5",
    "babel-plugin-component": "^1.1.1",
    "mockjs": "^1.0.1-beta3",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "airbnb-base"
    ],
    "plugins": [
      "vue"
    ],
    "rules": {
      "eqeqeq": 1,
      "no-plusplus": 0,
      "no-mixed-operators": 0,
      "max-len": [
        1,
        {
          "code": 120
        }
      ],
      "no-shadow": 0,
      "prefer-const": 1,
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "objects": "always-multiline",
          "imports": "always-multiline",
          "exports": "always-multiline",
          "functions": "ignore"
        }
      ],
      "import/extensions": 0,
      "no-param-reassign": [
        "error",
        {
          "props": true,
          "ignorePropertyModificationsFor": [
            "config",
            "e",
            "el",
            "v",
            "vm",
            "state"
          ]
        }
      ],
      "import/no-extraneous-dependencies": 0,
      "no-debugger": 1,
      "import/no-unresolved": 0,
      "no-console": 1,
      "no-multi-spaces": 0,
      "quote-props": 0,
      "vue/valid-v-for": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
